<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sleepswell">

<permission android:name="USE_FULL_SCREEN_INTENT"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SleepsWell">
        <activity android:name=".ui.Alarme.despertador"></activity>
        <activity
            android:name=".bottom_navigation"
            android:label="@string/title_activity_bottom_navigation" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".ui.Alarme.AlarmReceiver"/>
    </application>

</manifest>